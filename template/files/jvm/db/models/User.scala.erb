package <%= @base_package %>.jvm.db.models

import pye.db._

case class User private[db] (
    // Credentials
    username: String,
    encryptedPassword: String,
    // Info
    firstName: String,
    lastName: String,
    email: String,
) extends DbObject

object User {

  case class Session private[db] (
      userId: Long,
      token: String,
  ) extends DbObject

}
